{
    "name": "endroid/image-placeholder",
    "description": "Endroid Image Placeholder",
    "keywords": ["endroid", "image", "placeholder", "unsplash", "placehold", "placekitten", "php"],
    "homepage": "https://github.com/endroid/image-placeholder",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-gd": "*",
        "endroid/installer": "^1.2.2"
    },
    "require-dev": {
        "endroid/quality": "dev-main",
        "twig/twig": "^3.0"
    },
    "suggest": {
        "roave/security-advisories": "Avoids installation of package versions with vulnerabilities"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\ImagePlaceholder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\ImagePlaceholder\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "endroid/installer": true
        }
    }
}
